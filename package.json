{
	"name": "nest-server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "cross-env NODE\\_ENV=dev nest start --watch",
		"start:debug": "NODE_ENV=dev nest start --debug --watch",
		"start:prod": "cross-env NODE\\_ENV=production node dist/main",
		"start:pm2": "cross-env NODE\\_ENV=production pm2 start dist/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/apollo": "^10.1.0",
		"@nestjs/axios": "^0.1.0",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/graphql": "^10.1.1",
		"@nestjs/mercurius": "^10.1.0",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-socket.io": "^9.3.7",
		"@nestjs/websockets": "^9.3.7",
		"@types/dotenv": "^8.2.0",
		"apollo-server-express": "^3.10.2",
		"axios": "^1.2.0",
		"cheerio": "^1.0.0-rc.12",
		"class-validator": "^0.13.2",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.2",
		"firebase": "^9.10.0",
		"firebase-admin": "^11.5.0",
		"googleapis": "^107.0.0",
		"graphql": "^16.6.0",
		"nestjs-firebase": "^8.2.3",
		"puppeteer": "^19.3.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.1.2",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.5.0",
		"@types/node": "^18.7.16",
		"@types/socket.io": "^3.0.2",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "28.0.3",
		"json-loader": "^0.5.7",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.1",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.8.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
